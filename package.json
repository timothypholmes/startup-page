{
  "name": "start-page",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@react-three/fiber": "^7.0.29",
    "@vitejs/plugin-react": "^1.2.0",
    "apexcharts": "^3.33.0",
    "axios": "^0.25.0",
    "dayjs": "^1.10.7",
    "react": "^17.0.2",
    "react-apexcharts": "^1.3.9",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-router-dom": "^6.2.1",
    "three": "^0.137.5"
  },
  "scripts": {
    "deploy": "npm run build:tailwind && npm run build && gh-pages -d dist",
    "start": "vite --open",
    "build": "vite build",
    "preview": "vite preview",
    "serve": "vite build && serve -s dist -p 8000",
    "build:tailwind": "tailwindcss -i ./src/assets/styles/index.css -o ./src/assets/styles/tailwind.css",
    "build:css": "postcss src/styles/index.css -o /src/assets/styles/tailwind.css"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.2",
    "gh-pages": "^4.0.0",
    "postcss": "^8.4.7",
    "tailwindcss": "^3.0.23",
    "vite": "^2.9.13",
    "vite-plugin-string": "^1.1.2"
  }
}
